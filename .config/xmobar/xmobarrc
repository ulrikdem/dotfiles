Config
    { template = "}%xmonad%{%dynnetwork%%wi%%cpu%%cpufreq%%memory%%alsa:default:Master%%battery%%date%"
    , fgColor = "gray50"
    , commands =
        [ Run Wireless ""
            [ "-t", "<fc=,><ssid></fc> <fn=1>󰖩</fn> <fc=gray25><raw=1:│/></fc>"
            , "-x", "<raw=38:"
            ] 10
        , Run DynNetwork
            [ "-t", " <fn=1>󰇚</fn> <rx>K <fn=1>󰕒</fn> <tx>K <fc=gray25>│</fc>"
            , "-h", "green4"
            , "-l", "orange3"
            , "-H", "0"
            , "-L", "0"
            ] 10
        , Run Cpu
            [ "-t", " <fn=0></fn> <fn=2><vbar></fn>"
            , "-h", "red3,gray25"
            , "-n", "orange3,gray25"
            , "-l", "green4,gray25"
            , "-H", "75"
            , "-L", "50"
            ] 10
        , Run CpuFreq
            [ "-t", " <fc=green4><max></fc>GHz <fc=gray25>│</fc>"
            , "-d", "1"
            ] 10
        , Run Memory
            [ "-t", " <fn=0></fn> <fn=2><usedvbar></fn> <fc=gray25>│</fc>"
            , "-h", "red3,gray25"
            , "-n", "orange3,gray25"
            , "-l", "green4,gray25"
            , "-H", "75"
            , "-L", "50"
            ] 10
        , Run Alsa "default" "Master"
            [ "-t", " <fn=1><status></fn> <fc=gray25>│</fc>"
            , "--"
            , "-O", "<fc=gray50>󰕾</fc>"
            , "-o", "<fc=gray50>󰖁</fc>"
            ]
        , Run Battery
            [ "-t", " <fn=1><leftbar></fn><acstatus> <fc=gray25>│</fc>"
            , "-h", "green4"
            , "-n", "orange3"
            , "-l", "red3"
            , "-H", "50"
            , "-L", "25"
            , "-W", "0"
            , "-f", "󰂎󰁺󰁻󰁼󰁽󰁾󰁿󰂀󰂁󰂂󰁹"
            , "--"
            , "-i", "<fn=1>󱐋</fn>"
            , "-O", "<fn=1>󱐋</fn>"
            , "-o", " <fc=gray50><timeleft></fc>"
            ] 10
        , Run Date " %Y-%m-%d %H:%M:%S " "date" 10
        ]
    }

